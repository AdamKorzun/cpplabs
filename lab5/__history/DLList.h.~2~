//---------------------------------------------------------------------------

#ifndef DLListH
#define DLListH
//---------------------------------------------------------------------------
#endif
#include "Queue.h"
class DLList : private Queue{
	void Swap(){
		int maxEl;
		int minEl;
		Node* temp = Tail;
		temp=Head;
		while(temp != NULL){
			if (temp->x > maxEl) {
				maxEl = temp->x;
			}
			else if (temp->x < minEl) {
                minEl = temp->x;
			}
			temp = temp->next;
		}
	}
};